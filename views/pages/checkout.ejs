<!DOCTYPE html>
<html lang="zxx">

    <%- include("../blocks/head") %> 

<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header Section Begin -->
    <%- include("../blocks/header") %> 
    <!-- Header End -->

    <!-- Breadcrumb Section Begin -->
    <div class="breacrumb-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb-text product-more">
                        <a href="./"><i class="fa fa-home"></i> Home</a>
                        <a href="./shop">Shop</a>
                        <span>Thanh toán</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Breadcrumb Section Begin -->

    <!-- Shopping Cart Section Begin -->
    <section class="checkout-section spad">
        <div class="container">
            <form action="#" class="checkout-form">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="checkout-content">
                            <a href="#" class="content-btn">Đăng nhập</a>
                        </div>
                        <h4>Chi tiết hóa đơn</h4>
                        <div class="row">
                            <div class="col-lg-6">
                                <label for="fir">Họ<span>*</span></label>
                                <input type="text" id="fir">
                            </div>
                            <div class="col-lg-6">
                                <label for="last">Tên<span>*</span></label>
                                <input type="text" id="last">
                            </div>
                            <div class="col-lg-12">
                                <label for="cun-name">Tên công ty</label>
                                <input type="text" id="cun-name">
                            </div>
                            <div class="col-lg-12">
                                <label for="cun">Địa chỉ<span>*</span></label>
                                <input type="text" id="cun">
                            </div>
                           
                            <div class="col-lg-6">
                                <label for="email">Địa chỉ email<span>*</span></label>
                                <input type="text" id="email">
                            </div>
                            <div class="col-lg-6">
                                <label for="phone">Số điện thoại<span>*</span></label>
                                <input type="text" id="phone">
                            </div>
                           
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="checkout-content">
                            <input type="text" placeholder="Nhập mà giảm giá">
                        </div>
                        <div class="place-order">
                            <h4>Sản phẩm</h4>
                            <div class="order-total">
                                <ul class="order-table">
                                    <li>Sản phẩm <span>Tổng</span></li>
                                    <li class="fw-normal">Combination x 1 <span>140.000 VNĐ</span></li>
                                    <li class="fw-normal">Combination x 1 <span>140.000 VNĐ</span></li>
                                    <li class="fw-normal">Combination x 1 <span>140.000 VNĐ</span></li>
                                    <li class="fw-normal">Subtotal <span>200.000 VNĐ</span></li>
                                    <li class="total-price">Tổng <span>720.000 VNĐ</span></li>
                                </ul>
                                <div class="payment-check">
                                    <div class="pc-item">
                                        <label for="pc-paypal">
                                            Thanh toán qua MoMo
                                            <input type="checkbox" id="pc-paypal">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                </div>
                                <div class="order-btn">
                                    <button type="submit" class="site-btn place-btn">Thanh toán</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </section>
    <!-- Shopping Cart Section End -->

    <!-- Partner Logo Section Begin -->
    <%- include("../blocks/footer") %> 
    <!-- Footer Section End -->

    <!-- Js Plugins -->
   <%- include("../blocks/js-page") %>
</body>

</html>