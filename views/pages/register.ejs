<!DOCTYPE html>
<html lang="zxx">

    <%- include("../blocks/head") %> 

<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header Section Begin -->
    <%- include("../blocks/header") %> 
    <!-- Header End -->

    <!-- Breadcrumb Section Begin -->
    <div class="breacrumb-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb-text">
                        <a href="/"><i class="fa fa-home"></i>Trang chủ</a>
                        <span>Đăng ký</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Breadcrumb Form Section Begin -->

    <!-- Register Section Begin -->
    <div class="register-login-section spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 offset-lg-3">
                    <div class="register-form">
                        <h2>Đăng ký</h2>
                        <form action="#">
                            <div class="group-input">
                                <label for="con-pass">Nhập tài khoản <span style="color: red;">*</span></label>
                                <input type="text" id="username">
                            </div>
                            <div class="group-input">
                                <label for="con-pass">Nhập mật khẩu <span style="color: red;">*</span></<label>
                                <input type="text" id="pass">
                            </div>
                            <div class="group-input">
                                <label for="con-pass">Nhập lại mật khẩu <span style="color: red;">*</span></label>
                                <input type="text" id="con-pass">
                            </div>
                            <button type="submit" class="site-btn register-btn">Đăng Ký</button>
                        </form>
                        <div class="switch-login">
                            <a href="./login" class="or-login">Hoặc Đăng Nhập</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Register Form Section End -->
    
    <!-- Partner Logo Section Begin -->
    <%- include("../blocks/footer") %> 
    <!-- Footer Section End -->

    <!-- Js Plugins -->
   <%- include("../blocks/js-page") %>
</body>

</html>